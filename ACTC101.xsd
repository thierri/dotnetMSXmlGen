<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ACTC101"
	xmlns="http://www.cip-bancos.org.br/ARQ/ACTC101.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:cat="http://www.cip-bancos.org.br/catalogomsg"
	targetNamespace="http://www.cip-bancos.org.br/ARQ/ACTC101.xsd"
	elementFormDefault="qualified" version="1.6.4">	
	
	<!-- Definição dos Tipos Globais -->
	
  <xs:annotation>
    <xs:documentation>
      Definição de tipos globais do SEC - CTC.
    </xs:documentation>
  </xs:annotation>

  <!-- Definição do ACTCDOC do arquivo PRO -->
  <xs:complexType name="ACTCDOCPROComplexType">
    <xs:sequence>
      <xs:element name="BCARQ" type="BCARQComplexType" />
    </xs:sequence>
  </xs:complexType>

  <!-- Definição do ACCCDOC do arquivo ERR -->
  <xs:complexType name="ACTCDOCERRComplexType">
    <xs:sequence>
      <xs:element name="BCARQ" type="BCARQERRComplexType" />
    </xs:sequence>
  </xs:complexType>

  <!-- Definição do BCARQ do arquivo -->
  <xs:complexType name="BCARQComplexType">
    <xs:sequence>
      <xs:element name="NomArq" type="Nome">
        <xs:annotation>
          <xs:documentation>
              Nome do Arquivo
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumCtrlEmis" type="ControleIF">
        <xs:annotation>
          <xs:documentation>
              Número de Controle do Emissor
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumCtrlDestOr" type="ControleIF" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
              Número de Controle do Destinatário Original
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ISPBEmissor" type="CNPJBase">
        <xs:annotation>
          <xs:documentation>
              ISPB do Emissor
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ISPBDestinatario" type="CNPJBase">
        <xs:annotation>
          <xs:documentation>
              ISPB do Destinatário
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DtHrArq" type="DataHora">
        <xs:annotation>
          <xs:documentation>
              Data e Hora de envio
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SitReq" type="SitReq" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
              Situação Requisição
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Grupo_Seq" type="Grupo_SeqComplexType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
              Grupo Sequência
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DtRef" type="Data">
        <xs:annotation>
          <xs:documentation>
              Data de Referência
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Grupo_SeqComplexType">
    <xs:sequence>
      <xs:element name="NumSeq" type="NumSeq">
        <xs:annotation>
          <xs:documentation>
            Número Sequência
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IndrCont" type="Indr">
        <xs:annotation>
          <xs:documentation>
            Indicador Continuidade
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- Definição do BCARQ do arquivo ERR -->
  <xs:complexType name="BCARQERRComplexType">
    <xs:sequence>
      <xs:element name="NomArq" type="NomArqCodErr_ComplexType">
        <xs:annotation>
          <xs:documentation>
            Nome do Arquivo
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- Definição do ESTARQ do arquivo -->
  <xs:simpleType name="ESTARQSimpleType">
    <xs:annotation>
      <xs:documentation>
        Copia do conteudo das informacoes de estimulo,
        quando houverem.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="512000" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Definição do tipos do evento e com atributo para retorno de codigo de erro -->

  <xs:simpleType name="AnoMes">
    <xs:annotation>
      <xs:documentation>
        Ano e mês no formato AAAA-MM, onde AAAA = ano, MM = mês
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{4}-\d{2}"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="AnoMes_CodErro">
    <xs:annotation>
      <xs:documentation>
            Ano e mês no formato AAAA-MM, onde AAAA = ano, MM = mês com atributo para retorno de codigo de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="AnoMes">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name='Agencia'>
    <xs:annotation>
      <xs:documentation>
          Número da agência da instituição que atua no sistema financeiro, sem dígito verificador.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:integer'>
      <xs:totalDigits value='4'/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="Agencia_CodErro">
    <xs:annotation>
      <xs:documentation>
            Número da agência da instituição que atua no sistema financeiro, 
            sem dígito verificador com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="Agencia">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CEP">
    <xs:annotation>
      <xs:documentation>
        CEP
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="8" />
      <xs:maxLength value="8" />
      <xs:pattern value="\d{8}"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CEP_CodErro">
    <xs:annotation>
      <xs:documentation>
        CEP com código de erro
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CEP">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CidEnd">
    <xs:annotation>
      <xs:documentation>
        Cidade do endereço
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="50" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CidEnd_CodErro">
    <xs:annotation>
      <xs:documentation>
        Cidade do endereço com código de erro
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CidEnd">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CNPJ">
    <xs:annotation>
      <xs:documentation>
        Numero de Identificação da Pessoa Jurídica no
        Cadastro Nacional de Pessoa Jurídica.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{14}" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CNPJ_CodErro">
    <xs:annotation>
      <xs:documentation>
        Numero de Identificação da Pessoa Jurídica no
        Cadastro Nacional de Pessoa Jurídica com código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CNPJ">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CNPJ_CPF">
    <xs:annotation>
      <xs:documentation>
        Numero de Identificação da Pessoa Jurídica no
        Cadastro Nacional de Pessoa Jurídica ou o Número
        no Cadastro de Pessoa Física. No caso de
        transferência para pessoas isentas de CNPJ ou
        CPF, o campo CNPJ ou CPF deve ser preenchido com zero.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{11}||\d{14}" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="CNPJ_CPF_CodErro">
    <xs:annotation>
      <xs:documentation>
        Número de identificação da Pessoa Juridica no Cadastro Nacional de Pessoa Jurídica 
        ou o Número no Cadastro de Pessoa Física com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CNPJ_CPF">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CNPJBase">
    <xs:annotation>
      <xs:documentation>
        Primeiras 8 posições do CNPJ.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{8}"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="CNPJBase_CodErro">
    <xs:annotation>
      <xs:documentation>
         CNPJ base com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CNPJBase">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodContrtoOr">
    <xs:annotation>
      <xs:documentation>
          Código do contrato original
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="40" />
      <xs:maxLength value="40" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="CodContrtoOr_CodErro">
    <xs:annotation>
      <xs:documentation>
            Código do contrato original com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodContrtoOr">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CodContrtoSCR">
    <xs:annotation>
      <xs:documentation>
        Código de identificação do contrato SCR
        Código alfanumérico interno da IF identificador do contrato.
        Deverá ser preenchido exatamente com o mesmo conteúdo que é informado no layout SCR 3040
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="40" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="CodContrtoSCR_CodErro">
    <xs:annotation>
      <xs:documentation>
        Código SCR do contrato com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodContrtoSCR">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  
  <xs:simpleType name="CodEvtTar">
    <xs:annotation>
      <xs:documentation>
        Código de evento de tarifa
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="4"/>
      <xs:maxLength value="4"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="CodEvtTar_CodErro">
    <xs:annotation>
      <xs:documentation>
        Código de evento de tarifa com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodEvtTar">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name='CodIF'>
    <xs:annotation>
      <xs:documentation>
        Código IF utilizado na compensação.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:string'>
      <xs:pattern value='\d{3}'/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="CodIF_CodErro">
    <xs:annotation>
      <xs:documentation>
            Código IF utilizado na compensação com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodIF">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name='CodMoeda'>
    <xs:annotation>
      <xs:documentation>
        Código da moeda no padrão CNAB.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:integer'>
      <xs:totalDigits value='2'/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="CodMoeda_CodErro">
    <xs:annotation>
      <xs:documentation>
            Código da moeda no padrão CNAB com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CodMoeda">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name='CondcDivgte'>
    <xs:annotation>
      <xs:documentation>
        Condição de Divergência
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:integer'>
      <xs:totalDigits value='3'/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name='CondcDivgte_CodErro'>
    <xs:annotation>
      <xs:documentation>
        Condição de Divergência com atributo para código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CondcDivgte">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="ControleIF">
    <xs:annotation>
      <xs:documentation>
        Número de Controle da IF
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="20" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="ControleIF_CodErro">
    <xs:annotation>
      <xs:documentation>
            Número único de controle gerado pela IF com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="ControleIF">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="CPF">
    <xs:annotation>
      <xs:documentation>
        Número no Cadastro de Pessoa Física
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{11}" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="CPF_CodErro">
    <xs:annotation>
      <xs:documentation>
        Número no Cadastro de Pessoa Física com código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CPF">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name='CtBancaria'>
    <xs:annotation>
      <xs:documentation>
        Número da conta bancária, com dígito verificador.
        Se o dígito verificador for alfa, devera ser substituido por "0" (zero).
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:integer'>
      <xs:totalDigits value='13'/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="CtBancaria_CodErro">
    <xs:annotation>
      <xs:documentation>
        Número da conta bancária, com dígito verificador com atributo para retorno de codigo de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="CtBancaria">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="Data">
    <xs:annotation>
      <xs:documentation>
        Data no formato AAAA-MM-DD. Onde
        AAAA = ano,
        MM = mes
        [01-12],
        DD = dia [01-31].
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date">
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="Data_CodErro">
    <xs:annotation>
      <xs:documentation>
            Data com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="Data">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="DataHora">
    <xs:annotation>
      <xs:documentation>
        Data e hora no formato AAAA-MM-DDThh:mm:ss. 
        Onde
          AAAA = ano,
          MM = mes [01-12],
          DD = dia [01-31],
          hh = horas [00-23],
          mm = minutos [00-59],
          ss = segundos [00-59].
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:dateTime">
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="DataHora_CodErro">
    <xs:annotation>
      <xs:documentation>
            Data e hora no formato AAAA-MM-DDThh:mm:ss com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="DataHora">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="Email">
    <xs:annotation>
      <xs:documentation>
        Email do Cliente
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="5" />
      <xs:maxLength value="80" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="Email_CodErro">
    <xs:annotation>
      <xs:documentation>
            Email com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="Email">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="EnqdmntContrto">
    <xs:annotation>
      <xs:documentation>
        Enquadramento do Contrato
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:totalDigits value="2"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="EnqdmntContrto_CodErro">
    <xs:annotation>
      <xs:documentation>
        Enquadramento do contrato com código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="EnqdmntContrto">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="FluxoPagto">
    <xs:annotation>
      <xs:documentation>
        Fluxo de Pagamento
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="1"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="FluxoPagto_CodErro">
    <xs:annotation>
      <xs:documentation>
            Fluxo de Pagamento com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="FluxoPagto">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="Indr">
    <xs:annotation>
      <xs:documentation>
        Indicador 'Sim' ou 'Nao'
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="S" />
      <xs:enumeration value="N" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="Indr_CodErro">
    <xs:annotation>
      <xs:documentation>
            Indicador 'Sim' ou 'Nao' com atributo para retorno de código de erro.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="Indr">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="IndRemun">
    <xs:annotation>
      <xs:documentation>
        Índice Remuneração
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:totalDigits value="2"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="IndRemun_CodErro">
    <xs:annotation>
      <xs:documentation>
        Índice Remuneração com código de erro
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="IndRemun">
        <xs:attribute name="CodErro" type="CodErro" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  
  <xs:simpleType name='MtvCanceltPortldd'>
    <xs:annotation>
      <xs:documentation>
        Motivo de Cancelamento da Portabilidade.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:integer'>
      <xs:totalDigits value='3'/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="MtvCanceltPortldd_CodErro">
    <xs:annotation>
      <xs:documentation>
        Motivo de Cancelamento da Portabilidade com código de erro.
      </xs:documentation>
